{
  "name": "take-home-test",
  "version": "1.0.0",
  "repository": "git@github.com:inato/take-home-test.git",
  "author": "inato",
  "license": "MIT",
  "private": true,
  "engines": {
    "yarn": ">=1.7.0",
    "node": "=16.x"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "npm run lint -- --fix",
    "test": "mocha --require ts-node/register --extensions ts tests 'tests/**/*.ts' --exit",
    "test-inspect": "mocha --inspect-brk --require ts-node/register --extensions ts tests 'tests/**/*.ts' --exit",
    "test-watch": "mocha --require ts-node/register --watch --watch-files src --watch-files tests --extensions ts tests 'tests/**/*.ts'",
    "start": "node -r ts-node/register src/trial.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "date-fns": "^2.28.0"
  }
}
